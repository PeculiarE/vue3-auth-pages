{
    "extends": [
      "plugin:vue/vue3-essential",
      "prettier"
    ],
    "rules": {
      "vue/no-unused-vars": "error",
      "comma-dangle": 0,
      "no-console": "error",
      "complexity": ["error", 5],
      "consistent-return": 0,
      "camelcase": 0,
      "no-param-reassign": 0,
      "linebreak-style": 0,
      "no-underscore-dangle": 0,
      "object-curly-newline": 0,
      "arrow-parens": 1
    }
}
